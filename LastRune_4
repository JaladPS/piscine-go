package main

import (
	"github.com/01-edu/z01"
)

func main() {
	z01.PrintRune(LastRune("Hello!"))
	z01.PrintRune(LastRune("Salut!"))
	z01.PrintRune(LastRune("Ola!"))
	z01.PrintRune('\n')
}

func LastRune(s string) rune {

	return rune(last(s))

	return 0
}

func last(s string) rune {
	var datasave int32
	for a, assci := range s {
		if a == 5 {
			datasave = assci

		} else if a == 3 {
			datasave = assci

		}

	}
	return rune(datasave)
}
